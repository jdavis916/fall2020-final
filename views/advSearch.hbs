<form action="/indivCar" method="POST">
            <select name="brands">
                <option value=""></option>
                {{#each brands}}
                    <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>

            <select name="models">
                <option value=""></option>
                {{#each models}}
                    <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>

            <select name="mpg">
                <option value=""></option>
