                {{/each}}
            </select>

            <select name="fuel">
                <option value=""></option>
                {{#each fuel}}
                    <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>

            <select name="door">
                <option value=""></option>
                {{#each door}}
                    <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>
            <select name="seat">
                <option value=""></option>
                {{#each seat}}
                    <option value="{{this}}">{{this}}</option>
                {{/each}}
            </select>
            <input type="submit" value="Submit">
        </form>
